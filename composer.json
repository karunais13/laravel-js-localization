{
    "name": "karu/simple-js-localization",
    "description": "Laravel Simple JS localization ",
    "type": "library",
    "keywords": ["localization", "laravel", "translation", "js"],
    "authors": [
        {
            "name": "Karunaiswaran",
            "email": "karunais1392@gmail.com"
        }
    ],
    "license" : "MIT",
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support" : ">= 5.0.0 <= 5.8.8",
        "illuminate/routing": ">= 5.0.0 <= 5.8.8",
        "illuminate/console": ">= 5.0.0 <= 5.8.8",
        "illuminate/config": ">= 5.0.0 <= 5.8.8"
    },
    "require-dev" : {
        "phpunit/phpunit" : ">=4.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Karu\\SimpleJsLocalization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karu\\SimpleJsLocalization\\LocalizationProvider"
            ]
        }
    }
}
