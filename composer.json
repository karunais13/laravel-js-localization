    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "5.*",
        "tedivm/jshrink": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0",
        "mockery/mockery": "^1.2@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Karunaiswaran",
            "email": "karunais1392@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
